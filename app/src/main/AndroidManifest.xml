<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.nitikaaggarwal.usernotes">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".ListActivity"
            android:theme="@style/AppTheme.WithoutActionBar"
            android:screenOrientation="portrait" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
            </activity>


        <activity android:name=".EditNoteActivity"
            android:theme="@style/AppTheme.WithoutActionBar"
            android:screenOrientation="portrait" >
            </activity>


        <activity android:name=".ShowNoteActivity" />



        <activity android:name=".AddNoteActivity"
            android:theme="@style/AppTheme.WithoutActionBar"
            android:screenOrientation="portrait" >
        </activity>
    </application>

</manifest>